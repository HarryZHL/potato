{
  "name": "@toystory/potato",
  "version": "0.1.1",
  "author": "zhanghelong",
  "description": "a component of pdf to html, base on Vue3.",
  "keywords": [
    "framework",
    "Vue3",
    "admin"
  ],
  "module": "./dist/potato.es.js",
  "main": "./dist/potato.umd.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/potato.es.js",
      "require": "./dist/potato.umd.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint \"src/**/*.{js,vue}\"",
    "build": "vite build --mode production",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:style": "stylelint --cache --fix \"src/**/*.{vue,less,postcss,css,sass,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:pretty": "pretty-quick --staged"
  },
  "dependencies": {
    "@element-plus/icons-vue": "2.1.0",
    "@vueuse/core": "^10.5.0",
